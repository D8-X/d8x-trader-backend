{
	"name": "history",
	"version": "1.0.0",
	"main": "src/main.ts",
	"files": [
		"dist",
		"src"
	],
	"license": "MIT",
	"scripts": {
		"build": "npx prisma generate --schema ../utils/prisma/schema.prisma && rimraf ./dist && tsc",
		"prisma-reset": "npx prisma migrate reset --force",
		"prisma-dev": "npx prisma migrate dev",
		"prisma-prod": "npx prisma migrate deploy",
		"run-production-reset": "yarn prisma-reset && yarn prisma-prod && node ./dist/main.js",
		"run-production": "yarn prisma-prod && node ./dist/main.js",
		"watch": "nodemon ./src/main.ts",
		"postinstall": "patch-package"
	},
	"dependencies": {
		"@d8x/perpetuals-sdk": "^2.1.31-alpha2",
		"@prisma/client": "^5.17.0",
		"prisma": "^5.17.0",
		"@types/ws": "^8.5.5",
		"cors": "^2.8.5",
		"cron-schedule": "^4.0.0",
		"date-fns": "^2.30.0",
		"ethers": "^6.13.1",
		"express": "^4.19.2",
		"node-fetch": "^3.3.1",
		"sturdy-websocket": "^0.2.1",
		"ts-node": "^10.9.1",
		"utils": "*",
		"winston": "^3.8.2"
	},
	"devDependencies": {
		"nodemon": "^3.1.4",
		"cron-schedule": "^4.0.0"
	}
}
